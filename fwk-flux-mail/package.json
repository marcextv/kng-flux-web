{
  "name": "fwk_flux_mail",
  "version": "1.0.0",
  "description": "Flux Mail Services",
  "main": "dist/backend/index.js",
  "scripts": {
    "start": "node dist/backend/index.js | bunyan",
    "start:dev": "npm run build && node dist/backend/index.js | bunyan",
    "debug": "node",
    "build": "npm run babelrc:backend && babel app/backend -d dist/backend",
    "babelrc:backend": "cp .babelrc_backend .babelrc",
    "build:watch": "node_modules\\.bin\\watch 'npm run build'  src/",
    "test": "npm run babelrc:backend && mocha mocha test/**/*.js --require babel-polyfill --compilers js:babel-register",
    "test:debug": "mocha test/**/*.js --require babel-polyfill --compilers js:babel-register --debug-brk",
    "copy": "cpx \"app/backend/**/*.marko\" dist/backend",
    "web": "npm run web:build",
    "babelrc:web": "cp .babelrc_web .babelrc"
  },
  "keywords": [
    "push"
  ],
  "author": "mdtorres",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "cpx": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-cleanhtml": "^1.0.3",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^2.3.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-uglify": "^3.0.2",
    "kTask": "^1.3.0",
    "mocha": "^3.2.0",
    "watch": "^1.0.1"
  },
  "dependencies": {
    "async": "^2.1.5",
    "bluebird": "^3.5.0",
    "btoa": "^1.1.2",
    "bunyan": "^1.8.9",
    "fwk-node": "^1.0.7",
    "http": "0.0.0",
    "http-proxy": "^1.17.0",
    "http-proxy-agent": "^2.0.0",
    "https": "^1.0.0",
    "https-proxy-agent": "^2.1.1",
    "json-parser": "^1.1.5",
    "jsonwebtoken": "^7.3.0",
    "marko": "^4.15.4",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.1.1",
    "url": "^0.11.0",
    "urlsafe-base64": "^1.0.0",
    "web-push": "^3.2.5"
  }
}
